{
  "name": "preconditions",
  "author": "Cory Parrish",
  "main": "./lib/preconditions.js",
  "version": "2.0.3",
  "description": "Support for Guava like Precondition error checking in Node.js",
  "homepage": "https://github.com/corybill/preconditions",
  "bugs": {
    "url": "https://github.com/corybill/preconditions/issues"
  },
  "scripts": {
    "test": "npm run ut && npm run lint",
    "ut": "./node_modules/maddox/bin/maddox.js ./spec/unit",
    "perf": "env perf=true env test=false maddox ./spec/unit",
    "uap": "env perf=true test=true maddox ./spec/unit",
    "coverage": "./bin/maddox-cov.js",
    "lint": "./node_modules/eslint/bin/eslint.js ./spec/ ./lib",
    "docs": "npm run preconditions && npm run instance && npm run singleton && npm run decorator && npm run validation",
    "preconditions": "./node_modules/jsdoc-to-markdown/bin/cli.js \"./lib/preconditions.js\" > api/preconditions.md",
    "instance": "./node_modules/jsdoc-to-markdown/bin/cli.js \"./lib/err/instance-validator.js\" > api/instance.md",
    "singleton": "./node_modules/jsdoc-to-markdown/bin/cli.js \"./lib/err/singleton-validator.js\" > api/singleton.md",
    "decorator": "./node_modules/jsdoc-to-markdown/bin/cli.js \"./lib/errr/decorator.js\" > api/decorator.md",
    "validation": "./node_modules/jsdoc-to-markdown/bin/cli.js \"./lib/errr/validator.js\" > api/validation.md"
  },
  "keywords": [
    "preconditions",
    "Guava",
    "Throw Error",
    "Errr",
    "Error",
    "Error Factory",
    "Error Builder",
    "Stack Trace",
    "Appendable stack trace",
    "append stack trace",
    "concat strack trace",
    "concatenate strack trace",
    "Appendable error",
    "append error",
    "concat error",
    "concatenate error",
    "Debug Params",
    "Promise",
    "Promise Chain",
    "errr"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/corybill/preconditions.git"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chance": "^1.0.1",
    "eslint": "^2.4.0",
    "jsdoc-to-markdown": "^1.3.3",
    "maddox": "0.0.8"
  },
  "dependencies": {
    "errr": "1.0.7",
    "lodash": "3.0.0"
  }
}
